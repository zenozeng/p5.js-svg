    "@typescript-eslint/parser": "^5.59.8",
    "chai": "^4.3.4",
    "eslint": "^8.42.0",
    "jsdoc": "^4.0.2",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-firefox-launcher": "^0.1.6",
